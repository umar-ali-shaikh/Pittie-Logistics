gsap.utils.toArray(".tech-stack .tech-card"); const CARD_HEIGHT = 300; const GAP
= 50; const TAB = 28; /* INITIAL NORMAL STATE */ cards.forEach(card => {
gsap.set(card, { position: "relative", y: 0 }); }); const tl = gsap.timeline({
scrollTrigger: { trigger: ".tech-stack", start: "top top", end: () =>
`+=${cards.length * 100}%`, // ðŸ‘ˆ each card gets scroll space scrub: true, pin:
true, pinSpacing: true } }); /* ACTIVATE ABSOLUTE STACK AT START */ tl.add(() =>
{ cards.forEach((card, i) => { gsap.set(card, { position: "absolute", top: 0,
left: 0, y: i * (CARD_HEIGHT + GAP), zIndex: cards.length - i }); }); }); /* ðŸ”¥
ONE BY ONE STACK */ cards.forEach((card, i) => { tl.to(card, { y: i * TAB, //
28px step zIndex: cards.length + i, ease: "none" }); }); // Prathamesh js
gsap.registerPlugin(ScrollTrigger); const cards =
gsap.utils.toArray(".tech-stack .tech-card"); const CARD_HEIGHT = 300; const GAP
= 50; const TAB = 28; function initTechStack() {
ScrollTrigger.getAll().forEach(st => st.kill()); gsap.killTweensOf(cards); const
isMobile = window.innerWidth <= 900; // Reset cards.forEach(card => {
gsap.set(card, { position: "relative", y: 0, clearProps: "transform" }); });
const tl = gsap.timeline({ scrollTrigger: { trigger: ".tech-stack", start:
isMobile ? "top+=80 top" : "top top", end: `+=${cards.length * 120}%`, scrub:
true, pin: true, pinSpacing: !isMobile, // ðŸ”¥ KEY FIX anticipatePin: 1 } }); //
Activate stack AFTER trigger starts tl.add(() => { cards.forEach((card, i) => {
gsap.set(card, { position: "absolute", top: 0, left: 0, width: "100%", y: i *
(CARD_HEIGHT + GAP), zIndex: cards.length - i }); }); }); // Stack animation
cards.forEach((card, i) => { tl.to(card, { y: i * TAB, ease: "none" }); }); } //
Init initTechStack(); // Re-init on resize window.addEventListener("resize", ()
=> { clearTimeout(window.__stackResize); window.__stackResize =
setTimeout(initTechStack, 300); }); Partners Page

