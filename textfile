var(--bg-sections); border-radius: 8px; display: flex; align-items: center;
justify-content: center; margin-bottom: 12px; } /* icon image smaller than
background */ .warehouse-card .icon img { width: 30px; height: 30px; object-fit:
contain; display: block; } .warehouse-card h4 { color: var(--bg-danger);
font-size: 20px; font-weight: 600; margin-bottom: 6px; } .warehouse-card p {
color: var(--text-primary); margin: 0; } /* Image Card */ .warehouse-image {
